{
    "name": "xisodev/laravel-pagebuilder",
    "description": "드래그 앤 드롭을 활용한 AMUZCMS 전용 페이지빌더입니다.",
    "keywords": [
        "laravel",
        "pagebuilder"
    ],
    "homepage": "https://github.com/XisoDev/Laravel-Pagebuilder",
    "license": "MIT",
    "authors": [
        {
            "name": "xiso",
            "email": "xiso@amuz.co.kr"
        },
        {
            "name": "Hans Schouten",
            "email": "hans@falcosolutions.nl"
        }
    ],
    "require": {
        "php": ">=7.0",
        "XisoDev/phpagebuilder": "^0.8"
    },
    "autoload": {
        "psr-4": {
            "XisoDev\\LaravelPageBuilder\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "XisoDev\\LaravelPageBuilder\\ServiceProvider"
            ],
            "aliases": {
                "PageBuilder": "XisoDev\\LaravelPageBuilder\\Facade"
            }
        }
    }
}
